  "main": "Backend/Server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node Backend/Server.js",
    "start:dev": "concurrently \"nodemon --ignore 'Frontend/news-scraper*'\" \"npm run Frontend\"",
    "client": "cd Frontend/news-scraper && npm run start",
    "install": "cd Frontend/news-scraper && npm install",
    "build": "cd Frontend/news-scraper && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
